{
    "name": "cainance",
    "version": "1.0.36",
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "license": "MIT",
    "devDependencies": {
        "@grpc/grpc-js": "^1.6.1",
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.23",
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.17.0",
        "eslint": "^8.12.0",
        "eslint-plugin-jest": "^26.1.3",
        "grpc_tools_node_protoc_ts": "^5.3.2",
        "grpc-tools": "^1.11.2",
        "jest": "^27.5.1",
        "prettier": "^2.6.2",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.1.4",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "@influxdata/influxdb-client": "^1.24.0",
        "@solana/web3.js": "^1.43.4",
        "dotenv": "^16.0.1",
        "ethers": "^5.6.8",
        "google-protobuf": "^3.20.1-rc.1",
        "mongoose": "^6.3.5",
        "mysql2": "^2.3.3",
        "sequelize": "^6.18.0",
        "tslib": "^2.3.1"
    },
    "scripts": {
        "build": "tsc -p tsconfig.json && cp src/proto/*.d.ts dist/proto",
        "dev": "yarn build && node dist/index.js",
        "start": "yarn build && node dist/index.js",
        "clean": "rimraf coverage dist tmp",
        "prebuild": "npm run lint",
        "build:watch": "tsc -w -p tsconfig.json",
        "build:release": "npm run clean && tsc -p tsconfig.release.json",
        "lint": "eslint ./src --ext .ts",
        "test": "jest --coverage",
        "test:watch": "jest --watch",
        "fmt": "prettier --write .",
        "protoc-ts": "cd src && protoc --plugin=protoc-gen-ts=../node_modules/.bin/protoc-gen-ts --ts_out=./ ./proto/*.proto",
        "protoc": "cd src && grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./ --grpc_out=grpc_js:./ ./proto/*.proto && yarn protoc-ts"
    }
}
