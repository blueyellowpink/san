		"google-protobuf": "^3.20.0",
		"http-errors": "~1.6.3",
		"joi": "^17.6.2",
		"jsonwebtoken": "^8.5.1",
		"md5": "^2.3.0",
		"mongoose": "^6.6.5",
		"morgan": "~1.9.1",
		"node-2fa": "^2.0.3",
